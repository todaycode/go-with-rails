package models

import (
	"fmt"
)

func buildIdsHolder(n int) (idsHolder string) {
	idsHolder = "$1"
	for i := 2; i <= n; i++ {
		idsHolder += fmt.Sprintf(",$%d", i)
	}
	return
}

func allKeys(am map[string]interface{}) []string {
	keys := make([]string, len(am))
	i := 0
	for k := range am {
		keys[i] = k
		i++
	}
	return keys
}
